<?xml version="1.0"?>
<package>
  <name>mongodb_media_server</name>
  <version>0.2.1</version>
  <description>The mongodb_media_server package provides a web based interface for 
  storing and retrieving media files in a mongodb database.</description>

  <maintainer email="cburbridge@gmail.com">Chris Burbridge</maintainer>

  <license>MIT</license>


  <buildtool_depend>catkin</buildtool_depend>

  <build_depend>rospy</build_depend>
  <build_depend>mongodb_store</build_depend>

  <run_depend>rospy</run_depend>
  <run_depend>mongodb_store</run_depend>
  <run_depend>python-webpy</run_depend>
  <run_depend>python-pymongo</run_depend>
  <run_depend>mongodb</run_depend>


  <export>  </export>
</package>
